                        <ul class="l-dropdown--mobile">
                            <li>
                                <a href="#">All Homes</a>
                            </li>
                            <li>
                                <a href="#">New Construction</a>
                            </li>
                            <li>
                                <a href="#">Forclosures</a>
                            </li>
                            <li>
                                <a href="#">Land</a>
                            </li>
                            <li>
                                <a href="#">Recently Sold</a>
                            </li>
                        </ul>
                    </li>
                    <li>
                        <a href="#">Sell</a>
                    </li>
                    <li>
                        <a href="#">Mortgage</a>
                    </li>
                    <li class="has-dropdown">
                        <span class="has-dropdown__title">Agents</span>
                        <ul class="l-dropdown--mobile">
                            <li>
                                <a href="#">Looking for an Agent</a>
                            </li>
                            <li>
                                <a href="#">Advertising Solutions</a>
                            </li>
                            <li>
                                <a href="#">Create Agent Profile</a>
                            </li>
                        </ul>
                    </li>
                </ul>
            </div>
        </div>
